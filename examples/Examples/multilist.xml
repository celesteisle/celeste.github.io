<?xml version="1.0" encoding="UTF-8"?>
<!-- Multi List Example
–––––––––––––––––––––––––––––––––––––––––––––––––– -->
<!-- When the content needs to be split up use seperate sections -->
<!-- All sections are REQUIRED to have a title attribute with a value -->
<!-- color is an optional attribute if you feel associating a color with the information makes sense-->
<!-- Available colors are red, green, blue, orange, yellow, pink, salmon, and lightblue -->
<!-- width is another optional attribute. Try to avoid using it if at all possible -->
<!-- width accepts values 0-12 -->
<content>
    <section title ='Title' color='red'>
        <data type='ulist'>
            <item>List item 1</item>
            <item>List item 2
                <data type='olist'>
                    <item>Sublist item 1</item>
                    <item>Sublist item 2</item>
                    <item>Sublist item 3</item>
                </data>
            </item>
            <item>List item 3</item>
        </data>
    </section>
    <section title='Title'>
        <data type='ulist'>
            <item>List item 1</item>
            <item>List item 2</item>
            <item>List item 3</item>
        </data>
    </section>
</content>